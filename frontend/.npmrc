# NPM registry configuration as fallback
registry=https://registry.npmjs.org/

# Network configuration
fetch-timeout=300000
fetch-retry-mintimeout=10000
fetch-retry-maxtimeout=60000
fetch-retry-factor=2
fetch-retries=3

# Cache settings
cache-max=300
cache-min=1

# Disable package-lock for yarn compatibility
package-lock=false

# SSL configuration
strict-ssl=true